
export function map{{name}}DetailsToInput(details: Partial<{{>detailsTypeName}}> | null): {{inputTypeInfo.typeName}} {
    const inputDefaults = getDefault{{name}}InputBase();
    
    if(details){
        for(const index in inputDefaults){
            if(Object.hasOwnProperty.call(details, index) && 
                Object.hasOwnProperty.call(inputDefaults, index)){
                (inputDefaults[index as keyof typeof inputDefaults] as any) = details[index as keyof typeof details]
            }
        }
    }

    return inputDefaults
}
