
export function map{{name}}PartialInputToInput(partialInput?: Partial<{{inputTypeInfo.typeName}}> | null): {{inputTypeInfo.typeName}} {
    const inputDefaults = getDefault{{name}}InputBase();
    
    if(partialInput){
        for(const index in inputDefaults){
            if(Object.hasOwnProperty.call(partialInput, index) && 
                Object.hasOwnProperty.call(inputDefaults, index)){
                (inputDefaults[index as keyof typeof inputDefaults] as any) = partialInput[index as keyof typeof partialInput]
            }
        }
    }

    return inputDefaults
}
